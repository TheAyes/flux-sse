		"README.md",
		"LICENSE"
	],
	"optionalDependencies": {
		"express": "^4.18.2"
	},
	"devDependencies": {
		"typescript": "^5.1.3",
		"@types/express": "^4.16.0",
		"uglify-js": "^3.17.4"
	}
}
